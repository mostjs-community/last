{
  "name": "most-last",
  "version": "2.0.0",
  "description": "Emit the last value of your streams",
  "main": "lib/index.js",
  "module": "lib.es2015/index.js",
  "jsnext:main": "lib.es2015/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "type-definitions"
  ],
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es2015 && npm run build:umd",
    "build:commonjs": "./node_modules/.bin/tsc -P cfg/tsconfig.commonjs.json",
    "build:es2015": "./node_modules/.bin/tsc -P cfg/tsconfig.es2015.json",
    "build:umd": "rollup -c && uglifyjs dist/most-last.js -o dist/most-last.min.js",
    "test": "./node_modules/.bin/eslint src/ && ./node_modules/.bin/mocha --require babel-core/register ",
    "preversion": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mostjs-community/last.git"
  },
  "keywords": [
    "most.js",
    "reactive",
    "streams",
    "last",
    "observable"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mostjs-community/last/issues"
  },
  "homepage": "https://github.com/mostjs-community/last#readme",
  "config": {
    "ghooks": {
      "commit-msg": "node ./node_modules/.bin/validate-commit-msg"
    }
  },
  "devDependencies": {
    "@most/core": "^1.2.3",
    "@most/scheduler": "^1.1.0",
    "@most/types": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.26.0",
    "buba": "^4.0.2",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.5",
    "power-assert": "^1.4.1",
    "rollup": "^0.36.0",
    "rollup-plugin-typescript2": "^0.13.0",
    "typescript": "^2.8.1",
    "uglify-js": "^2.7.3"
  },
  "peerDependencies": {
    "@most/core": "^1.2.3"
  },
  "dependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.6"
  }
}
